# 開発環境用設定（Firebase Cloud Development Environment）
# このファイルは開発環境デプロイ時に使用

# 環境識別
NODE_ENV=development
NUXT_ENV=development
FIREBASE_ENV=development

# Firebase開発環境設定
# 注意: 実際の値はCI/CDパイプラインで設定されます
FIREBASE_PROJECT_ID=${FIREBASE_DEV_PROJECT_ID}
FIREBASE_API_KEY=${FIREBASE_DEV_API_KEY}
FIREBASE_AUTH_DOMAIN=${FIREBASE_DEV_AUTH_DOMAIN}
FIREBASE_STORAGE_BUCKET=${FIREBASE_DEV_STORAGE_BUCKET}
FIREBASE_MESSAGING_SENDER_ID=${FIREBASE_DEV_MESSAGING_SENDER_ID}
FIREBASE_APP_ID=${FIREBASE_DEV_APP_ID}

# API設定
API_URL=${API_URL_DEV}

# マルチテナント設定（開発環境用）
DEFAULT_TENANT_ID=dev-salon
ENABLE_MULTI_TENANT=true

# 開発環境機能フラグ
ENABLE_DEBUG_MODE=true
ENABLE_DEVTOOLS=true
ENABLE_MOCK_DATA=false
ENABLE_ANALYTICS=false

# 監視・ログ設定
ENABLE_ERROR_REPORTING=true
ENABLE_PERFORMANCE_MONITORING=true
LOG_LEVEL=debug

# セキュリティ設定（開発環境）
ENABLE_CSP=true
ENABLE_HTTPS_REDIRECT=false